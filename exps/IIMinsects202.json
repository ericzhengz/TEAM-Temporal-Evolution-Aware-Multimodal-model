{
    "prefix": "iiminsects202_B20",
    "dataset": "iiminsects202",
    "memory_size": 120,
    "memory_per_class": 40,
    "fixed_memory": false,
    "device": [0],
    "seed": [42],
    "model_name": "proof",
    "convnet_type": "clip",
    "tuned_epoch": 20,
    "lr_scheduler": "cosine",
    "init_cls": 2,
    "increment": 2,
    "model_dir": "logs/iiminsects202",
    "batch_size": 16,
    "weight_decay": 5e-5,
    "init_lr": 0.004,
    "min_lr": 0,
    "milestones": [200, 280],
    "gamma": 0.2,
    "momentum": 0.9, 
    "shuffle": true,
    "balanced": true,
    "projection_type": "pure_mlp",
    "context_prompt_length_per_task": 10,
    "stage_first_replay": true,
    "exemplars_per_class": 5,
    "exemplar_selection_strategy": "herding",
    "update_state_distance_interval": 1,
    "state_distance_decay_factor": 0.9,
    "unicl_instance_weight": 0.6,
    "unicl_category_weight": 0.4,
    "temperature": 0.05,
    "zoom": 0.15,
    "optimizer": "adam",

    "temporal_enabled": true,
    "temporal_gcn_layers": 3,
    "temporal_gcn_hidden_dim": 256,
    "lifecycle_modeling_enabled": true,
    "lifecycle_weight": 0.4,
    "temporal_relation_threshold": 0.2,
    "class_lifecycle_consistency": 0.7,
    "temporal_feature_weight": 0.25,
    "state_evolution_steps": 5
}